<?xml version="1.0" encoding="UTF-8"?>
<component name="org.nuxeo.rss.feed.types">
  <extension target="org.nuxeo.ecm.platform.types.TypeService"
    point="types">

    <type id="RssFeed">
      <label>RssFeed</label>
      <icon>/icons/note.gif</icon>
      <bigIcon>/icons/note_100.png</bigIcon>
      <category>SimpleDocument</category>
      <description>syndication flux</description>
      <default-view>view_documents</default-view>
      <layouts mode="any">
        <layout>heading</layout>
        <layout>rss_diplay</layout>
      </layouts>
    </type>

    <type id="Workspace">
      <subtypes>
        <type>RssFeed</type>
      </subtypes>
    </type>

  </extension>
</component>