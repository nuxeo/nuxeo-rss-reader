<?xml version="1.0" encoding="UTF-8"?>
<component name="org.nuxeo.rss.feed.types.core">

  <extension target="org.nuxeo.ecm.core.schema.TypeService"
    point="schema">
    <schema name="rss-feed" src="schemas/rss-feed.xsd" prefix="rf" />
  </extension>

  <extension target="org.nuxeo.ecm.core.schema.TypeService"
    point="doctype">
    <doctype name="RssFeed" extends="Document">
      <schema name="common" />
      <schema name="rss-feed" />
      <schema name="uid" />
      <schema name="dublincore" />
    </doctype>
  </extension>

  <extension target="org.nuxeo.ecm.core.schema.TypeService"
    point="doctype">
    <doctype name="RssFeedRoot" extends="OrderedFolder"/>
  </extension>

</component>